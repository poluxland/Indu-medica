


<div class="row">
  <div class="col-sm-6">
    <h1>Equipos</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_equipo_path, class: 'btn btn-primary' do %>
    Nuevo Equipo
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table id="dt" class="table table-striped table-bordered table-hover">
    <thead>
      <tr>


            
            <th>Año</th>
            <th>Serie</th>
            <th>Gas</th>
            <th>Voltaje</th>
            <th>Oficinas</th>
            <th>Nombre oficinas</th>
            <th>Unidad master</th>
            <th>Cliente</th>

          <th></th>

      </tr>
    </thead>

    <tbody>
      <% @equipos.each do |equipo| %>
        <%= content_tag :tr, id: dom_id(equipo), class: dom_class(equipo) do %>



                      <td><%= equipo.año %></td>
                      <td><%= equipo.serie %></td>
                      <td><%= equipo.gas %></td>
                      <td><%= equipo.voltaje %></td>
                      <td><%= equipo.oficinas %></td>
                      <td><%= equipo.nombre_oficinas %></td>
                      <td><%= equipo.unidad_master %></td>
                      <td><%= equipo.cliente.nombre %></td>


            <td><%= link_to 'Ver', equipo %></td>

        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
